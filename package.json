{
  "name": "person-manager",
  "version": "0.1.0",
  "homepage": "https://roberts98.github.io/person-manager",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "axios": "^0.19.2",
    "concurrently": "^5.2.0",
    "gh-pages": "^2.2.0",
    "json-server": "^0.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start:client": "react-scripts start",
    "start:api": "json-server https://randomuser.me/api?results=10&inc=name,location,email,login,picture",
    "start": "concurrently --kill-others-on-fail \"yarn start:client\" \"yarn start:api\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3000"
}
